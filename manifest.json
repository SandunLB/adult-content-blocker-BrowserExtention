{
  "manifest_version": 3,
  "name": "Advanced Content Blocker",
  "version": "1.0",
  "description": "Blocks specific URLs for adult content and redirects to a warning page.",
  "permissions": [
    "declarativeNetRequest",
    "declarativeNetRequestWithHostAccess",
    "storage",
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "assets/icon.png",
    "128": "assets/icon128.png"
  }
}
